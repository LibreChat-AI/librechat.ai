# PostHog analytics host URL (optional, public)
NEXT_PUBLIC_POSTHOG_HOST=
# PostHog project API key (optional, public)
NEXT_PUBLIC_POSTHOG_KEY=

# Crisp live chat widget ID (optional, public)
