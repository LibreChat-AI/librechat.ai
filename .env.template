# Supabase project URL (optional)
SUPABASE_URL=
# Supabase service role key for server-side access (optional, secret)
SUPABASE_SERVICE_ROLE_KEY=
# Langfuse observability keys (optional)
NEXT_PUBLIC_LANGFUSE_PUBLIC_KEY=
LANGFUSE_PUBLIC_KEY=
LANGFUSE_SECRET_KEY=

